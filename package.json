{
  "name": "gs-colorpicker",
  "version": "1.0.0",
  "description": "a color pikcer for GS web applications",
  "main": "./dist/gs-colorpicker.js",
  "types": "./dist/gs-colorpicker.d.ts",
  "scripts": {
    "build": "export NODE_OPTIONS=--openssl-legacy-provider && webpack --config webpack.prod.js",
    "build:dev": "export NODE_OPTIONS=--openssl-legacy-provider &&  webpack --display-used-exports --display-reasons --config webpack.dev.js",
    "build:docs": "export NODE_OPTIONS=--openssl-legacy-provider && webpack --config webpack.docs.js",
    "start:docs": "export NODE_OPTIONS=--openssl-legacy-provider && webpack-dev-server --open --open-page index.html --port 3000 --config webpack.docs.js",
    "test": "export NODE_OPTIONS=--openssl-legacy-provider && rm -rf node_modules package-lock.jsonwebpack-dev-server --open --open-page test.html --port 3000 --config webpack.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alkadia/a-color-picker.git"
  },
  "keywords": [
    "javascript",
    "color",
    "picker",
    "color-picker"
  ],
  "author": "Alkadia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Alkadia/a-color-picker/issues"
  },
  "homepage": "https://github.com/Alkadia/a-color-picker/",
  "devDependencies": {
    "del": "^8.0.1",
    "gulp": "^5.0.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-rename": "^2.1.0",
    "gulp-terser": "^2.1.0",
    "html-loader": "^0.5.5"
  }
}
